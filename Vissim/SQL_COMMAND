SELECT
CAST(ARG_TIMEINTERVAL AS BIGINT) AS Time,
STOPDELAY, sum(STOPDELAY) OVER (ORDER BY CAST(ARG_TIMEINTERVAL AS BIGINT)) as CumStopDelay,
VEHDELAY, sum(VEHDELAY) OVER (ORDER BY CAST(ARG_TIMEINTERVAL AS BIGINT)) as CumDelay
FROM DELAYMEASUREMENT_EvaluationTimeIntervalClass




SELECT
CAST(ARG_TIMEINTERVAL AS BIGINT) AS Time,
DELAYSTOPTOT, sum(DELAYSTOPTOT) OVER (ORDER BY CAST(ARG_TIMEINTERVAL AS BIGINT)) as CumStopDelay,
DELAYTOT, sum(DELAYTOT) OVER (ORDER BY CAST(ARG_TIMEINTERVAL AS BIGINT)) as CumDelay
FROM VEHICLENETWORKPERFORMANCEMEASUREMENT_EvaluationTimeIntervalClass